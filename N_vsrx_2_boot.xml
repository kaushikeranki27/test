security {
    screen {
        ids-option untrust-screen {
            icmp {
                ping-death;
            }
            ip {
                source-route-option;
                tear-drop;
            }
            tcp {
                syn-flood {
                    alarm-threshold 1024;
                    attack-threshold 200;
                    source-threshold 1024;
                    destination-threshold 2048;
                    queue-size 2000; ## Warning: 'queue-size' is deprecated
                    timeout 20;
                }
                land;
            }
        }
    }
    policies {
        from-zone trust to-zone trust {
            policy default-permit {
                match {
                    source-address any;
                    destination-address any;
                    application any;
                }
                then {
                    permit;
                }
            }
        }
        from-zone trust to-zone untrust {
            policy default-permit {
                match {
                    source-address any;
                    destination-address any;
                    application any;
                }
                then {
                    permit;
                }
            }
        }
        from-zone untrust to-zone trust {
            policy default-permit {
                match {
                    source-address any;
                    destination-address any;
                    application any;
                }
                then {
                    permit;
                }
            }
        }
    }
    zones {
        security-zone trust {
            tcp-rst;
            host-inbound-traffic {
                system-services {
                    http;
                    ping;
                    ntp;
                    snmp;
                    snmp-trap;
                    ssh;
                    netconf;
                    dhcp;
                }
            }
            interfaces {
                ge-0/0/1.0;
            }
        }
        security-zone untrust {
            screen untrust-screen;
            host-inbound-traffic {
                system-services {
                    http;
                    ping;
                    ntp;
                    snmp;
                    snmp-trap;
                    ssh;
                    netconf;
                }
            }
            interfaces {
                ge-0/0/7.0;
                ge-0/0/8.0;
            }
        }
    }
}
interfaces {
	ge-0/0/0 {
		description "Management interface"
        unit 0 {
            family inet {
                address 10.0.0.3/30;
            }
        }
    }
	$$LanInterface
    ge-0/0/1 {
		description "Lan interface"
        unit 0 {
			family inet{
				address $*serviceTwoIpAddress$/30
			}
        }
    }
	$$LanInterfaceEnds
	ge-0/0/2 {
		description "Lan interface"
        unit 0 {
        }
    }
	ge-0/0/3 {
		description "Lan interface"
        unit 0 {
        }
    }
	ge-0/0/4 {
		description "Lan interface"
        unit 0 {
        }
    }
	ge-0/0/5 {
		description "Lan interface"
        unit 0 {
        }
    }
	ge-0/0/6 {
		description "Lan interface"
        unit 0 {
        }
    }
	$$WanInterface
    ge-0/0/7 {
		description "Wan interface"
		flexible-vlan-tagging;
        unit 0 {
			vlan-id $*secondaryVlanId$;
            family inet {
                address $*secondaryCeIpAddress0$/30;
            }
        }
    }
	$$WanInterfaceEnds
    ge-0/0/8 {
		description "Wan interface"
        flexible-vlan-tagging;
        unit 0 {
            vlan-id $*primaryVlanId$;
            family inet {
                address $*primaryCeIpAddress0$/30;
            }
        }
    }
    fxp0 {
		description "Liveliness interface"
        unit 0 {
        }
    }
}
snmp {
    location Verizon;
    contact Verizon;
    community private {
        authorization read-write;
    }
    community public {
        authorization read-only;
    }
}
routing-options {
    static {
        route 0.0.0.0/0 next-hop $*serviceTwoIpAddress2$;
    }
    autonomous-system $*asNumber$;
}
protocols {
    bgp {
        traceoptions {
            file bgp-log;
            flag general;
            flag all;
        }
        remove-private;
        group as701-verizon {
            type external;
            neighbor $*serviceTwoIpAddress2${
                peer-as $*peerAs$;
            }
        }
    }
}